#include<stdio.h>
#include<string.h>
#include<stdlib.h>

void main()
{
    char str1[10],str2[10];
    int i=0,len,count=0;
    printf("Enter the first string:\n");
    scanf("%s",str1);
    printf("Enter the second string\n");
    scanf("%s",str2);
    len = strlen(str1);
    
    if(strlen(str1)!=strlen(str2))
    {
       printf("Not anagrams\n");
       exit(0);
    }
    else
    {
      while(str1[i]!='\0')
       {
         int j=0;
         while(str2[j]!='\0')
             {
                if(str1[i]==str2[j])
                {
                    count++;
                    break;
                }
                 else if(str1[i]!=str2[j] && j==len)
                {
                    printf("Not anagram\n");
                    exit(0);
                }
                j++;
             }
             i++;
         }
    }
    if(len==count)
    printf("Anagrams\n");
    else
    printf("Not anagrams\n");
}
